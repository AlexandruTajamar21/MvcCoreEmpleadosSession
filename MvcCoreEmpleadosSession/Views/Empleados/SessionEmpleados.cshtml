@model List<Empleado>

<h1>Session Empleados</h1>
<a asp-controller="Empleados" asp-action="EmpleadosAlmacenados">Empleados Almacenados Session</a>
<h2>@ViewData["MENSAJE"]</h2>

<ul class="list-group">
    @foreach(Empleado emp in Model)
    {
        <li class="list-group-item list-group-item-info">
            @emp.Apellido
            <a asp-controller="Empleados" asp-action="SessionEmpleados" asp-route-idEmpleado="@emp.EmpNo" class="btn btn-danger">Almacenar Empleado</a>
        </li>
    }
</ul>
