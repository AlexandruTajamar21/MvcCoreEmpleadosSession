<h1> Empleados almacenados Session</h1>

@if(Context.Session.GetString("EMPLEADOS")== null)
{
    <h1 style="color:red">No existen Empleados en Session</h1>
}
else
{
    List<Empleado> empleados = Context.Session.GetObject<List<Empleado>>("EMPLEADOS");
    <ul class="list-group">
        @foreach(Empleado emp in empleados)
                {
                    <li class="list-group-item list-group-item-info">
                        @emp.Apellido, @emp.Oficio
                    </li>
                }
    </ul>
}

